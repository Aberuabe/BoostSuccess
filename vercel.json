{
  "version": 2,
  "routes": [
    {
      "src": "/admin.js",
      "dest": "/admin.js"
    },
    {
      "src": "/app.js",
      "dest": "/app.js"
    },
    {
      "src": "/styles.css",
      "dest": "/styles.css"
    },
    {
      "src": "/(index|admin-login|admin-dashboard|payment)(\\.html)?",
      "dest": "/$1.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/server.js"
    }
  ]
}